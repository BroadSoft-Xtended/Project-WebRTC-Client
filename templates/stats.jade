.stats.fadeable.popup
  .statsContainer
  .table.fixed.collapse
    .heading
      .cell Video Stats
      .cell Audio Stats
    .row
      .cell
        | Bitrate out: 
        a.statsVar(href='javascript:;', data-type='video', data-var='kiloBitsSentPerSecond')
        |  kb/s
      .cell
        | Bitrate out: 
        a.statsVar(href='javascript:;', data-type='audio', data-var='kiloBitsSentPerSecond')
        |  kb/s
    .row
      .cell
        | Bitrate in: 
        a.statsVar(href='javascript:;', data-type='video', data-var='kiloBitsReceivedPerSecond')
        |  kb/s
      .cell
        | Bitrate in: 
        a.statsVar(href='javascript:;', data-type='audio', data-var='kiloBitsReceivedPerSecond')
        |  kb/s
    .row
      .cell
        | Lost: 
        a.statsVar(href='javascript:;', data-type='video', data-var='packetsLost')
        |  packets (
        a.statsVar(href='javascript:;', data-type='video', data-var='packetsLostPer')
        |  %)
      .cell
        | Lost: 
        a.statsVar(href='javascript:;', data-type='audio', data-var='packetsLost')
        |  packets (
        a.statsVar(href='javascript:;', data-type='audio', data-var='packetsLostPer')
        |  %)
    .row
      .cell
        | Frame rate out: 
        a.statsVar(href='javascript:;', data-type='video', data-var='googFrameRateSent')
        |  in: 
        a.statsVar(href='javascript:;', data-type='video', data-var='googFrameRateReceived')
      .cell
        | Audio Level out: 
        a.statsVar(href='javascript:;', data-type='audio', data-var='audioInputLevel')
        |  in: 
        a.statsVar(href='javascript:;', data-type='audio', data-var='audioOutputLevel')
    .row.spacer
    .heading
      .cell Resolution
      .cell Link
    .row
      .cell
        | In: 
        span.statsVar(data-type='video', data-var='googFrameWidthReceived')
        |  x 
        span.statsVar(data-type='video', data-var='googFrameHeightReceived')
        |  Out: 
        span.statsVar(data-type='video', data-var='googFrameWidthSent')
        |  x 
        span.statsVar(data-type='video', data-var='googFrameHeightSent')
      .cell
        span.statsDelay
          | Delay: 
          a.statsVar(href='javascript:;', data-type='audio', data-var='googRtt')
        |  Jitter: 
        a.statsVar(href='javascript:;', data-type='audio', data-var='googJitterReceived')
    .row.spacer


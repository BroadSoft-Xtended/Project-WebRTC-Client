.quality
  display none
  position absolute
  z-index 10
  top 13px
  left 15px

.icon-quality
  &:before
    color #00FF00
    opacity .4

.icon-quality1
  &:before
    content "\e007"
.icon-quality2
  &:before
    content "\e006"
.icon-quality3
  &:before
    content "\e005"
.icon-quality4
  &:before
    content "\e004"

.view-audioOnly .quality1, .view-audioOnly .quality2, .view-audioOnly .quality3, .view-audioOnly .quality4
	display none !important


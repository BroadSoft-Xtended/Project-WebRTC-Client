.bdsft-client
	.enableMute.sound-shown.started .muteAudioIcon,
	.enableMute:not(.sound-shown).started .unmuteAudioIcon
		{fadeIn}

	.enableMute:not(.started) .muteAudioIcon, 
	.enableMute:not(.started) .unmuteAudioIcon,
	.enableMute:not(.sound-shown).started .muteAudioIcon, 
	.enableMute.sound-shown.started .unmuteAudioIcon
		{fadeOut}
